{


  "header": {
    "tag": "div",
    "class": "header",
    "inner": [
      {
        "tag": "text",
        "class": "title",
        "id": "text1",
        "inner": "%text%"
      }
    ]
  },

   "main": {
    "tag": "div",
    "class": "main",
    "id": "main",
     "inner": [
       {
         "tag": "div",
         "class": "header"
       },

       {
         "tag": "div",
         "class": "items"
       },
       {
         "tag": "div",
         "class": "items-input"
       }
     ]

  },

  "nav": {

    "tag": "div",
    "class": "navigation",
    "id": "navigation",
    "inner": [
      {
        "tag": "text",
        "inner": "ToDo",
        "onclick": "%onclick%",
        "id":"todo"
      },
      {
        "tag": "text",
        "inner": "Done",
        "onclick": "%onclick2%",
        "id":"done"
      }
    ]

  },

  "item": {
    "tag": "div",
    "class": "item",
    "inner": [
      {
        "tag": "input",
        "type": "checkBox",
        "class": "checkBox",
        "onclick": "%onclick%",
        "id": "%id%"
      },
      {
        "tag": "div",
        "class": "text",
        "inner": "%text%"
      }
    ]

  },

  "input": {
    "tag": "div",
    "class": "input_item",
    "id": "items-input",
    "inner": [
      {
        "tag": "div",
        "class": "text",

        "inner": {
          "tag": "form",
          "inner": [
            {
              "tag": "input",
              "type": "text",
              "id": "input-text",
              "placeholder": "Enter message here..."
            },
            {
              "tag": "input",
              "type": "submit",
              "value": "hinzuf√ºgen"
            }
          ],
          "onsubmit": "%onsubmit%"
        }
      }
    ]
  }
}